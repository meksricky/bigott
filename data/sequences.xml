<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Gift Composition Sequence -->
        <record id="seq_gift_composition" model="ir.sequence">
            <field name="name">Gift Composition Sequence</field>
            <field name="code">gift.composition.sequence</field>
            <field name="prefix">GC</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Gift Experience Code Sequence -->
        <record id="seq_gift_experience_code" model="ir.sequence">
            <field name="name">Gift Experience Code</field>
            <field name="code">gift.experience.code</field>
            <field name="prefix">EXP</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Batch Processor Sequence -->
        <record id="seq_batch_processor" model="ir.sequence">
            <field name="name">Batch Composition Processor</field>
            <field name="code">batch.composition.processor</field>
            <field name="prefix">BATCH</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Assembly Sheet Sequence -->
        <record id="seq_assembly_sheet" model="ir.sequence">
            <field name="name">Gift Assembly Sheet</field>
            <field name="code">gift.assembly.sheet</field>
            <field name="prefix">ASM</field>
            <field name="suffix">-%(year)s-%(month)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Delivery Note Sequence -->
        <record id="seq_delivery_note" model="ir.sequence">
            <field name="name">Gift Delivery Note</field>
            <field name="code">gift.delivery.note</field>
            <field name="prefix">DN</field>
            <field name="suffix">-%(year)s-%(month)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Experience Pool Importer Sequence -->
        <record id="seq_experience_importer" model="ir.sequence">
            <field name="name">Experience Pool Import</field>
            <field name="code">experience.pool.importer</field>
            <field name="prefix">IMP</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>