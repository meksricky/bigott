<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Gift Composition Sequence -->
        <record id="seq_gift_composition" model="ir.sequence">
            <field name="name">Gift Composition</field>
            <field name="code">gift.composition</field>
            <field name="prefix">GC</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        
        <!-- Ollama Recommender Sequence -->
        <record id="seq_ollama_recommender" model="ir.sequence">
            <field name="name">Ollama Recommender</field>
            <field name="code">ollama.gift.recommender</field>
            <field name="prefix">OGR</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        
    </data>
</odoo>